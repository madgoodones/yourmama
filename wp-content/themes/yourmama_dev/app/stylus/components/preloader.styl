/* Preloader */
#preloader
  fixed: 0
  size: 100%
  background-color #000
  z-index 100
  &.desactived
    transition(1.3s all ease-in-out)
    opacity 0
    visibility hidden

#status
  width 100%
  height 100%
  position absolute
  background-repeat no-repeat
  background-position center
  background-size 280px